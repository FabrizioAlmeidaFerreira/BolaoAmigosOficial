<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fabrizio.bolaoamigosoficial">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserPalpitesActivity"
            android:label="@string/title_activity_user_palpites"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PalpitesActivity"
            android:label="@string/title_activity_palpites"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CadastreseActivity"></activity>
    </application>

</manifest>